@model Vidly.Models.Customer

@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>MemberShipType: @Model.MembershipType.MembershipName</li>
    @if (Model.BirthDate.HasValue)
    {
        <li>BirthDate: @Model.BirthDate</li>
    }
    
</ul>